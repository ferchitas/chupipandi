    <style name="AppThemeAM" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/cageta</item>
        <item name="colorPrimaryDark">@color/azulClaro</item>
