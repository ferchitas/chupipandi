        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/morado1</item>
        <item name="colorPrimaryDark">@color/morado</item>
        <item name="android:windowBackground">@color/verdeCesped</item>
    </style>

    <style name="AppThemeRV" parent="Theme.AppCompat.Light.DarkActionBar">
