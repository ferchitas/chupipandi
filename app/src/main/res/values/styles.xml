<resources>

    <!-- Base application theme. <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>-->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primaryN</item>
        <item name="colorPrimaryDark">@color/primary_darkN</item>
        <item name="android:windowBackground">@color/primary_lightN</item>
    </style>

    <style name="AppThemeRV" parent="Theme.AppCompat.Light.DarkActionBar">
