    </style>

    <style name="AppThemeAA" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primaryAA</item>
        <item name="colorPrimaryDark">@color/primary_darkAA</item>
        <item name="android:windowBackground">@color/primary_lightAA</item>
    </style>





    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
